  },
  "homepage": "https://github.com/Zubair-Ashraf/graphql-prisma#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "apollo-server": "^2.18.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "env-cmd": "^10.1.0",
    "graphql-yoga": "^1.14.10",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.4",
    "prisma-binding": "^2.3.16",
    "uuid": "^3.3.2"
